<?xml version="1.0" encoding="utf-8"?>
<odoo>

	<data>

		<record id="seq_student_student" model="ir.sequence">
			<field name="name">Student Sequence</field>
			<field name="code">student.student</field>
			<field name="prefix">STD/</field>
			<field name="padding">5</field>
		</record>

	</data>

	<data>
		<record id="seq_teacher_teacher" model="ir.sequence">
			<field name="name">Teacher Sequence</field>
			<field name="code">teacher.teacher</field>
			<field name="prefix">TCH/</field>
			<field name="padding">5</field>
		</record>
	</data>

	<data>
		<record id="seq_school_fees" model="ir.sequence">
			<field name="name">Fees Sequence</field>
			<field name="code">school.fees</field>
			<field name="prefix">FSS/</field>
			<field name="padding">5</field>
		</record>
	</data>




</odoo>